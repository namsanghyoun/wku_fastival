    <!--난장섹션임.-->
    <section class="success2" id="nanjang">
    <div class="container">
      <div class="row">
          <div class="col-lg-12 text-center">
              <h2><img src="/img/flo2.png"/>난장정보<img src="/img/flo2.png"/></h2>
              <hr class="star-light">
          </div>
         <div id="map2"></div>
         
         <div class="nanjang-div">
            <div class="nanjang3-div">
            <% for i in 0..3 %>
              <% for j in 3*i+1..3*i+3 %>
                  <div id="div<%= j%>-3"  class="nanjang3-div">
                      <div class="nanjang-img">
                        <% unless Nanjang.find(j).image == nil%>
                          <%= image_tag (Nanjang.find(j).image) %>
                        <% end %>
                      </div>
                      <div class="nanjang-name-div">
                        <span><%= Nanjang.find(j).name%></span>
                      </div>
                      <div class="nanjang-introduce-div">
                        <span>소개</span>
                        <%= Nanjang.find(j).introduce%>
                      </div>
                      
                    </div>
              <% end %>
            <% end %>
                
          </div>
            <% for i in 0..3 %>
              <div class="nanjang-row">
                  <% for j in 3*i+1..3*i+3 %>
                    <div class="nanjang2-div">
                      <p class="btn btn3" rel="div<%=j%>-3" style="color:<%=Nanjang.find(j).color%>; border-color:<%=Nanjang.find(j).color%>; min-width:90px;border-width:1.3px; padding:5px 0px" value="<%=j%>">
                        <%= Nanjang.find(j).name%>
                      </p>
                    </div>
              
                  <% end %>
              </div>
            <% end %>
          </div>
          
        </div>
      </div>
    </section>
    
<!--주점섹션임-->
    <section class="success3" id="jujum">
    <div class="container">
      <div class="row">
          <div class="col-lg-12 text-center">
              <h2><img src="/img/flo2.png"/>주점정보<img src="/img/flo2.png"/></h2>
              <hr class="star-light">
          </div>
      </div>
      <div class="row">
        <!--지도 섹션임-->
        <div id="map"></div>
        <!-- 주점정보 -->
        
        <div class="acc">
          <div class="like-reset">좋아요 수는 매일 초기화됩니다.</div>
          <% @jujums.each do |j| %>
            <% unless j.id == 24 %>
              <div class="accordion" id="acc-panel<%=j.id%>-btn">
                <div class="acc-name"><%= j.name%></div>
                <div class="acc-like-down">
                    <button class="like-btn"value="<%= j.id%>"><img src="/img/like.png"/></button>              
                    <span class="like-count"id="<%= j.id%>-count" value="<%= j.id%>"><%=@merong_count[j.id]%></span>
                    &#9662;
                </div>
              </div>
              <div class="panel" id="acc-panel<%= j.id%>" >
                
                <input type="number" id="<%= j.id%>-hidden"value="<%=@merong_count[j.id]%>" style="display:none;"/>
                <input type="hidden" value="<%= @merong_no[j.id]%>" id="<%=j.id%>-no"/>
                <% unless j.image == nil %>
                  <%=image_tag j.image %>
                <% end %>
                <div class="panel-name-div">
                  <span><%= j.name %></span>
                </div>
                <div class="panel-theme-div">
                  <span>테마</span>
                  <%= j.theme %>
                </div>
                <div class="panel-introduce-div">
                  <span>소개</span>
                  <%= j.introduce.html_safe %>
                </div>
                <div class="panel-menu-div"> 
                  <span>메뉴</span>
                  <% unless j.menu == nil%>
                    <%=image_tag j.menu %>
                  <% end %>
                </div>
              </div>
            <% else %>
              <div class="accordion" id="acc-panel<%=j.id%>-btn">
                <div class="acc-name"><%= j.name%></div>
                <div class="acc-like-down">
                    <button class="like-btn"value="<%= j.id%>"><img src="/img/like.png"/></button>              
                    <span class="like-count"id="<%= j.id%>-count" value="<%= j.id%>"><%=@merong_count[j.id]%></span>
                    &#9662;
                </div>
              </div>
              <div class="panel" id="acc-panel<%= j.id%>" >
                
                <input type="number" id="<%= j.id%>-hidden"value="<%=@merong_count[j.id]%>" style="display:none;"/>
                <input type="hidden" value="<%= @merong_no[j.id]%>" id="<%=j.id%>-no"/>
                <% unless j.image == nil %>
                  <%=image_tag j.image %>
                <% end %>
                <div class="panel-name-div">
                  <span><%= j.name %></span>
                </div>
                <div class="panel-theme-div">
                  <span>테마</span>
                  <%= j.theme %>
                </div>
                <div class="panel-introduce-div">
                  <span>소개</span>
                  <%= j.introduce.html_safe %>
                </div>
                <%= image_tag '/img/muse.jpg'%>
                <div class="panel-menu-div"> 
                  전주연합댄스팀 뮤즈(Muse)
                </div>
              </div>
            <% end %>
            
          <% end %>
          
        </div>
    </div>
    </section>



