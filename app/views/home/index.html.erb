<script>
  
//좋아요
 $(".like-btn").click(function(){
        var id = this.value;
        var ho = parseInt($("#"+id+"-hidden").val());
        var no = $("#"+id+"-no").val();
        var he = ho+1;
            if(no==0){
               $.ajax({
                    url:'/home/ho',
                    method:"get",
                    data:{"jujum_id":id},
                    success:function(data){
                        $("#"+id+"-count").text(he);
                    },
                    error:function(){
                     }
                })
            }
        });
  
  
    var mapContainer = document.getElementById('map2'), // 지도를 표시할 div 
        mapOption = { 
            center: new daum.maps.LatLng(35.967797, 126.956795), // 지도의 중심좌표
            level: 1 // 지도의 확대 레벨
        };  
    
    var map = new daum.maps.Map(mapContainer, mapOption); // 지도를 생성합니다
    
    var positions2 = [  // 마커의 위치
      {
         title: '몽수룸', 
         latlng: new daum.maps.LatLng(35.968017, 126.956940),
         sw: new daum.maps.LatLng(35.967977, 126.956896), // 사각형 영역의 남서쪽 좌표
         ne: new daum.maps.LatLng(35.968055, 126.956990),
         color:"#f09590"
      },
      {
         title: '온에어', 
         latlng: new daum.maps.LatLng(35.967922, 126.956897),
         sw: new daum.maps.LatLng(35.967879, 126.956853), // 사각형 영역의 남서쪽 좌표
         ne: new daum.maps.LatLng(35.967962, 126.956951),
         color:"#95A5C3"
      },
      {
        title: 'WITH', 
        latlng: new daum.maps.LatLng(35.967830, 126.956855),
         sw: new daum.maps.LatLng(35.967782, 126.956807), // 사각형 영역의 남서쪽 좌표
         ne: new daum.maps.LatLng(35.967865, 126.956910),
         color:"#7F9FA4"
      },
      {  
        title: '그린나래', 
        latlng: new daum.maps.LatLng(35.967726, 126.956835),
        sw: new daum.maps.LatLng(35.967685, 126.956782), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967764, 126.956887),
        color:"#deb29e"
      },
      {
        title: '원불교학과', 
        latlng: new daum.maps.LatLng(35.967635, 126.956790),
        sw: new daum.maps.LatLng(35.967590, 126.956740), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967667, 126.956845),
        color:"#bd9dc7"
      },
      {
        title: '백두', 
        latlng: new daum.maps.LatLng(35.967540, 126.956756),
        sw: new daum.maps.LatLng(35.967490, 126.956700), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967575, 126.956814),          
        color:"#e68b60"
      },
      {
        title: '행정학과', 
        latlng: new daum.maps.LatLng(35.968010, 126.956780),
        sw: new daum.maps.LatLng(35.967974, 126.956735), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.968050, 126.956840), 
        color:"#de625b"
      },
      {
        title: '원불교 대학생회', 
        latlng: new daum.maps.LatLng(35.967920, 126.956744),
        sw: new daum.maps.LatLng(35.967877, 126.956693), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967955, 126.956794), 
        color:"#887b8f"
      },
      {
        title: '소방행정학과', 
        latlng: new daum.maps.LatLng(35.967820, 126.956700),
        sw: new daum.maps.LatLng(35.967778, 126.956646), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967859, 126.956753), 
        color:"#b5bf8e"
      },
      {
        title: '여학생회', 
        latlng: new daum.maps.LatLng(35.967720, 126.956670),
        sw: new daum.maps.LatLng(35.967675, 126.956620), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967755, 126.956725), 
        color:"#8db5b0"
      },
      {
        title: 'GNC', 
        latlng: new daum.maps.LatLng(35.967620, 126.956625),
        sw: new daum.maps.LatLng(35.967570, 126.956570), // 사각형 영역의 남서쪽 좌표
        ne: new daum.maps.LatLng(35.967655, 126.956680), 
        color:"#c9a9ab"
      },
      
      ]
    
    for(var i=0, len=positions2.length; i<len;i++){
      
       // 지도에 표시할 사각형을 생성합니다
      var rectangleBounds = new daum.maps.LatLngBounds(positions2[i].sw, positions2[i].ne);
      var rectangle = new daum.maps.Rectangle({
          bounds: rectangleBounds, // 그려질 사각형의 영역정보입니다
          strokeWeight: 4, // 선의 두께입니다
          strokeColor: positions2[i].color, // 선의 색깔입니다
          strokeOpacity: 1, // 선의 불투명도 입니다 1에서 0 사이의 값이며 0에 가까울수록 투명합니다
          strokeStyle: 'solid', // 선의 스타일입니다
          fillColor: positions2[i].color, // 채우기 색깔입니다
          fillOpacity: 0.8 // 채우기 불투명도 입니다
      });
      rectangle.setMap(map); 
      
     
    }
    
    for(var i=0, len=positions2.length; i<len;i++){
      var customOverlay = new daum.maps.CustomOverlay({
          position: positions2[i].latlng,
          content: '<div class ="label" style="color:#000; border:none; border-radius:5px; background-color:#fff; opacity:0.7;"><span class="center">'+positions2[i].title+'</span></div>'
      });
      
      // 커스텀 오버레이를 지도에 표시합니다
      customOverlay.setMap(map);
      
    }
    
   
</script>

